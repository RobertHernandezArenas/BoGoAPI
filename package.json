{
	"name": "bogoapi",
	"version": "1.0.0",
	"description": "API for Android Mobile App",
	"homepage": "https://github.com/RobertHernandezArenas/BoGoAPI#readme",
	"bugs": {
		"url": "https://github.com/RobertHernandezArenas/BoGoAPI/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RobertHernandezArenas/BoGoAPI.git"
	},
	"license": "MIT",
	"author": "Robert Hernandez Arenas",
	"type": "module",
	"main": "app.js",
	"scripts": {
		"build": "rimraf lib && babel src --out-dir lib && node ./copyScript.js",
		"dev": "node --env-file=.env ./src/app.js",
		"lint": "eslint src/**/*.js --fix",
		"format": "prettier src/**/*.js --write",
		"db": "node initDb.js",
		"start": "nodemon --env-file=.env ./src/app.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"globals": "^16.0.0",
		"nodemon": "^3.1.4",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.8"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"env-var": "^7.5.0",
		"envs": "^0.1.5",
		"express": "^4.21.2",
		"express-session": "^1.18.1",
		"fs-extra": "^11.2.0",
		"handlebars": "^4.7.8",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.12.0",
		"morgan": "^1.10.0",
		"mysql2": "^3.10.2",
		"nodemailer": "^6.10.0",
		"sequelize": "^6.37.3",
		"stripe": "^17.7.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.13.0"
	}
